                "examples": {
                  "json": {
                    "summary": "test data",
                    "value": "{\n  \"success\": true,\n  \"data\": {\n    \"pages\": 938,\n    \"records\": [\n      {\n        \"id\": 565,\n        \"userName\": \"jerald.jacobson\",\n        \"userAccount\": \"q0zz5p\",\n        \"userAvatar\": \"qrlqwi\",\n        \"gender\": 1,\n        \"userRole\": \"4v9b36\",\n        \"createTime\": \"2023-03-12 20:24:36\",\n        \"updateTime\": \"2023-03-12 20:24:36\"\n      }\n    ],\n    \"total\": 270,\n    \"size\": 923,\n    \"current\": 383,\n    \"orders\": [\n      {\n        \"column\": \"a8qb3i\",\n        \"asc\": true\n      }\n    ],\n    \"optimizeCountSql\": true,\n    \"searchCount\": true,\n    \"optimizeJoinOfCountSql\": true,\n    \"countId\": \"177\",\n    \"maxLimit\": 694\n  },\n  \"code\": \"89517\",\n  \"message\": \"success\",\n  \"timestamp\": 1678623876670\n}"
